<div class="margin-top">
	 <h1>応募者一覧</h1>

	 <tbody>
			<% @recitments.each do |recitment| %>
				<tr>
					<td>
						<%= link_to recitment.user.nickname, recitment.user %>
					</td>
					<td>
						<%= recitment.pref.name %>
					</td>
					<td>
						<%= link_to recitment.title, recitment %>
					</td>
				</tr>
			<% end %>
		</tbody>
</div>